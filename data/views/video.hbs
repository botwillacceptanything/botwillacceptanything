{{#if data.mediaIsVideo}}
    
<iframe width="560" height="315" src="https://www.youtube.com/embed/{{data.mid}}?start={{data.time}}&autoplay=1" frameborder="0" allowfullscreen></iframe>

{{/if}}

{{#if data.mediaIsMusic}}

<iframe id="sciframe" width="560" height="315" scrolling="no" frameborder="no"
src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/{{data.mid}}&amp;auto_play=true&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false&amp;visual=true"></iframe>

{{/if}}
